#include <bits/stdc++.h> 
bool isPossibleToSurvive(int n, int x, int d){
    // Write your code here
    if(d <= 6){
        if(n >= x){
            return true;
        }else {
            return false;
        }
    }
    else{
        if(n >= x && 6*n >= 7*x){
            return true;
        }else {
            return false;
        }
    }
}
