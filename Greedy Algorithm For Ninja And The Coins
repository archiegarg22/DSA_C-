int minimumCoins(int v) {
    // Write your code here.
    int arr[9];
    int count = 0;
    int sum = v;
    arr[0]= 1000 ;
    arr[1]= 500 ;
    arr[2]= 100 ;
    arr[3]= 50 ;
    arr[4]= 20 ;
    arr[5]= 10 ;
    arr[6]= 5 ;
    arr[7]= 2 ;
    arr[8]= 1 ;

    

    for( int i = 0 ;i <9;i++){
         while( v>= arr[i]){
        
            count++;
            v = v - arr[i];
          
    }

    }

   
    return count;
}
